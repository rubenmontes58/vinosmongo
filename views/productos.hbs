{{{navbar}}}
<div class="container">
    <h3 class="h3centrado">NUESTROS PRODUCTOS</h3>
    <div>
        <img class="aligncenter size-full wp-image-663" src="../images/border-title.png" alt="" width="224" height="21">
        <br>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Producto</th>
                <th scope="col">tipo</th>
                <th scope="col">Descripcion</th>
                <th scope="col">Stock</th>
                <th scope="col">Costo</th>
                <th scope="col">Precio de Venta</th>
            </tr>
        </thead>
        <tbody>
            {{#each vinos}}
            <tr>
                <td>{{id}}</td>
                <td>{{producto}}</td>
                <td>{{tipo}}</td>
                <td>{{descripcion}}</td>
                <td>{{stock}}</td>
                <td>{{costo}}</td>
                <td>{{precioventa}}</td>
            </tr>
            {{/each}}
    </table>
</div>